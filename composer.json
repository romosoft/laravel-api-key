{
    "name": "leftsky/laravel-api-key",
    "description": "Laravel API密钥管理系统，支持API密钥的创建、验证和管理",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "leftsky",
            "email": "leftsky@vip.qq.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "suggest": {
        "filament/filament": "提供API密钥管理界面"
    },
    "autoload": {
        "psr-4": {
            "Leftsky\\LaravelApiKey\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leftsky\\LaravelApiKey\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leftsky\\LaravelApiKey\\ApiKeyServiceProvider"
            ],
            "aliases": {
                "ApiKey": "Leftsky\\LaravelApiKey\\Facades\\ApiKey"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 